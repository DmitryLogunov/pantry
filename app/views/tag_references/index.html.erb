<p id="notice"><%= notice %></p>

<h1>TagReferences</h1>

<table>
  <thead>
    <tr>
      <th>Tag</th>
      <th>Thing</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @tag_references.each do |tag_reference| %>
      <tr>
        <td><%= tag_reference.tag_id %></td>
        <td><%= tag_reference.thing_id %></td>
        <td><%= link_to 'Show', tag_reference %></td>
        <td><%= link_to 'Edit', edit_tag_reference_path(tag_reference) %></td>
        <td><%= link_to 'Destroy', tag_reference, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Tag', new_tag_reference_path %>
